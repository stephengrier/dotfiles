alias ll="ls -l"
alias fix-pinentry='gpg-connect-agent updatestartuptty /bye > /dev/null 2>&1'
alias mount-encrypted='/Applications/VeraCrypt.app/Contents/MacOS/VeraCrypt --text --mount /dev/disk4s1 /Volumes/veracrypt --slot 1'
alias unmount-encrypted='/Applications/VeraCrypt.app/Contents/MacOS/VeraCrypt --text --dismount --slot 1'
alias whatsmyip='curl ifconfig.me/ip'
